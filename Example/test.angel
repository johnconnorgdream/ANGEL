lib a
l = []
lsbytype(base,type) {
	ls = fls(base+'*.*');
	for(item in ls) {
		if(item['name'] == null)
		else if(item['name'].size() == 0)
		else if(item['name'][0] == '.')
		else if(item['attr'] == 'dir') {
			dir = base+item['name']+'\\'
			printl(dir)
			lsbytype(dir,type);
		}
	}
}
i = 0
dirdis = $lsbytype;
dirdis('c:\\windows\\',null)