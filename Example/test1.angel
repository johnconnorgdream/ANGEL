lib a
lsbytype(base,type) {
	printl(base)
	ls = fls(base+'*.*')
	for(item in ls) {
		if(item['name'] == null)
		else if(item['name'].size() == 0)
		else if(item['name'][0] == '.')
		else if(item['attr'] == 'dir') {
			dir = base+item['name']+'\\';
			.dirdis(dir,type);
		}
	}
}
